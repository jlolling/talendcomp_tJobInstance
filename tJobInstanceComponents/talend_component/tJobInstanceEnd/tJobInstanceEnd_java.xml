<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT>
	<HEADER AUTHOR="cimt AG" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" RELEASE_DATE="20140908A"
		SERIAL="" STARTABLE="true" STATUS="PROD" VERSION="3.3">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Management</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="USE_SEPARATE_CONNECTION" FIELD="CHECK" NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
    	<PARAMETER
	      NAME="CONNECTION"
	      FIELD="COMPONENT_LIST"
	      FILTER="REGEXP:^(.)*(Connection)(.)*"
	      NUM_ROW="5" 
	      REQUIRED="true"
	      SHOW_IF="USE_SEPARATE_CONNECTION == 'true'"
    	/>
		<PARAMETER NAME="JOB_START_COMP" FIELD="COMPONENT_LIST" FILTER="tJobInstanceStart"
			NUM_ROW="10" REQUIRED="true" />
		<PARAMETER NAME="JOB_RESULT" FIELD="TEXT" NUM_ROW="20" />
		<PARAMETER FIELD="TEXT" NAME="TIME_RANGE_START" NUM_ROW="22" />
		<PARAMETER FIELD="TEXT" NAME="TIME_RANGE_END" NUM_ROW="22" />
		<PARAMETER FIELD="TEXT" NAME="VALUE_RANGE_START" NUM_ROW="23" />
		<PARAMETER FIELD="TEXT" NAME="VALUE_RANGE_END" NUM_ROW="23" />
		<PARAMETER NAME="SAVE_NAMED_COUNTERS" FIELD="CHECK" NUM_ROW="25" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SAVE_CONTEXT_AT_END" FIELD="CHECK" NUM_ROW="25">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DO_NOT_SAVE_PASSWORDS" NUM_ROW="25" SHOW_IF="SAVE_CONTEXT_AT_END == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CLEANUP_BY_WORKITEM" FIELD="CHECK" NUM_ROW="26">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CLOSE_CONNECTION" FIELD="CHECK" NUM_ROW="26">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CHECK_ALL_COMPONENTS_FINISHED" FIELD="CHECK" NUM_ROW="28">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="INPUTS" FIELD="TABLE" NUM_ROW="30"
			NB_LINES="4">
			<ITEMS>
				<ITEM NAME="INPUT_COUNTER" FIELD="TEXT"/>
				<ITEM NAME="ADD" FIELD="CHECK" VALUE="true"/>
				<ITEM NAME="COUNTER_NAME" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="OUTPUTS" FIELD="TABLE" NUM_ROW="40"
			NB_LINES="4">
			<ITEMS>
				<ITEM NAME="OUTPUT_COUNTER" FIELD="TEXT"/>
				<ITEM NAME="ADD" FIELD="CHECK" VALUE="true"/>
				<ITEM NAME="COUNTER_NAME" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="UPDATES" FIELD="TABLE" NUM_ROW="50"
			NB_LINES="4">
			<ITEMS>
				<ITEM NAME="UPDATE_COUNTER" FIELD="TEXT"/>
				<ITEM NAME="ADD" FIELD="CHECK" VALUE="true"/>
				<ITEM NAME="COUNTER_NAME" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="REJECTS" FIELD="TABLE" NUM_ROW="60"
			NB_LINES="4">
			<ITEMS>
				<ITEM NAME="REJECT_COUNTER" FIELD="TEXT"/>
				<ITEM NAME="ADD" FIELD="CHECK" VALUE="true"/>
				<ITEM NAME="COUNTER_NAME" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="DELETES" FIELD="TABLE" NUM_ROW="70"
			NB_LINES="4">
			<ITEMS>
				<ITEM NAME="DELETE_COUNTER" FIELD="TEXT"/>
				<ITEM NAME="ADD" FIELD="CHECK" VALUE="true"/>
				<ITEM NAME="COUNTER_NAME" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS />
	<CODEGENERATION>
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="TIME_RANGE_START" TYPE="id_Date" />
		<RETURN AVAILABILITY="AFTER" NAME="TIME_RANGE_END" TYPE="id_Date" />
		<RETURN AVAILABILITY="AFTER" NAME="STRING_VALUE_RANGE_START" TYPE="id_String" />
		<RETURN AVAILABILITY="AFTER" NAME="STRING_VALUE_RANGE_END" TYPE="id_String" />
		<RETURN AVAILABILITY="AFTER" NAME="RETURN_CODE" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="RETURN_MESSAGE" TYPE="id_String" />
		<RETURN AVAILABILITY="AFTER" NAME="DELETED_PREVIOUS_JOB_INSTANCES" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="MEMORY_MAX_USED" TYPE="id_Long" />
		<RETURN AVAILABILITY="AFTER" NAME="MEMORY_MAX_USED_PERCENTAGE" TYPE="id_Double" />
		<RETURN AVAILABILITY="AFTER" NAME="MEMORY_AVAILABLE" TYPE="id_Long" />
	</RETURNS>
</COMPONENT>
