			NUM_ROW="3"
			REQUIRED="true" 
			SHOW_IF="(USE_DATA_SOURCE == 'true')">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

    	<PARAMETER
	      	NAME="CONNECTION"
	      	FIELD="COMPONENT_LIST"
